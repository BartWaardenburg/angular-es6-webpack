{
	"parser": "babel-eslint",
	"rules": {
		"indent": [1, "tab", {"SwitchCase": 1}],
		"quotes": [2, "single"],
		"linebreak-style": [2, "unix"],
		"semi": [2, "always"],
		"block-scoped-var": 2,
		"max-depth": [1, 4],
		"one-var": [1, "never"]
	},
	"globals": {
        "anwb": true,
        "Mustache": true,
        "_gaq": true
    },
	"env": {
		"browser": true,
		"commonjs": true,
		"es6": true
	},
	"extends": "eslint:recommended"
}
