{
  "name": "angular-es6-webpack",
  "version": "0.0.1",
  "description": "A styleguide/generator for starting up a single page application using Angular.js ES6 and Webpack at its core",
  "tags": ["angular", "ES6", "ES2015", "webpack", "tape", "styleguide", "karma", "jsdoc"],
  "license": "MIT",
  "scripts": {
    "prod": "webpack --config webpack.prod.js --bail -p && jsdoc src/  -c conf.json -r",
    "dev": "webpack-dev-server --history-api-fallback --progress --inline",
    "test": "karma start",
    "test:live": "karma start --auto-watch --no-single-run"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/BartWaardenburg/angular-es6-webpack"
  },
  "author": "Bart Waardenburg <bart.waardenburg@incentro.com>",
  "dependencies": {
    "angular": "^1.4.8",
    "angular-animate": "^1.4.8",
    "lodash": "^3.10.1"
  },
  "devDependencies": {
    "angular-mocks": "^1.4.8",
    "autoprefixer": "^6.2.3",
    "babel-core": "^6.3.26",
    "babel-eslint": "^4.1.6",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.3.13",
    "blue-tape": "^0.1.11",
    "css-loader": "^0.23.1",
    "eslint": "^1.10.3",
    "eslint-loader": "^1.1.1",
    "extract-text-webpack-plugin": "^0.9.1",
    "html-loader": "^0.4.0",
    "html-webpack-plugin": "^1.7.0",
    "isparta-instrumenter-loader": "^1.0.0",
    "karma": "^0.13.16",
    "karma-chrome-launcher": "^0.2.2",
    "karma-coverage": "^0.5.3",
    "karma-phantomjs-launcher": "^0.2.2",
    "karma-sourcemap-loader": "^0.3.6",
    "karma-spec-reporter": "0.0.23",
    "karma-tap": "^1.0.3",
    "karma-webpack": "^1.7.0",
    "less": "^2.5.3",
    "less-loader": "^2.2.2",
    "ng-annotate-loader": "0.0.10",
    "phantomjs": "^1.9.19",
    "style-loader": "^0.13.0",
    "tape": "^4.4.0",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0"
  }
}
